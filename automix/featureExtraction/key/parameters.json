{
    "sampleRate": 44100,
    "validFileTypes": [
        ".wav",
        ".mp3",
        ".flac",
        ".aiff",
        ".ogg"
    ],
    "highpassCutoff": 200,
    "spectralWhitening": true,
    "detuningCorrection": true,
    "detuningCorrectionScope": "average",
    "PCPThreshold": 0.2,
    "windowSize": 4096,
    "hopSize": 4096,
    "windowShape": "hann",
    "minHz": 25,
    "maxHz": 3500,
    "spectralPeaksThreshold": 0.0001,
    "spectralPeaksMax": 60,
    "HPCPBandPreset": false,
    "HPCPSplitHz": 250,
    "HPCPHarmonics": 4,
    "HPCPNonLinear": false,
    "HPCPNormalize": "none",
    "HPCPShift": false,
    "HPCPReferenceHz": 440,
    "HPCPSize": 12,
    "HPCPWeightWindowSemitones": 1,
    "HPCPWeightType": "cosine",
    "keyProfile": "bgate",
    "useThreeProfiles": true,
    "withModalDetails": true,
    "separator": " "
}